{
    "manifest_version": 3,
    "name": "VolaStocker",
    "version": "0.2.0",
    "author": "Norio4 Yahata",
    "description": "Simple Url Clip Box For Volatile Stock",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    }
}
